{
  "name": "seampass-password-generator",
  "version": "1.0.0",
  "description": "Secure password generation and strength checking",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "eslint . --max-warnings 0",
    "check-types": "tsc --noEmit"
  },
  "keywords": [
    "password",
    "generator",
    "security",
    "random",
    "memorable",
    "strength"
  ],
  "author": "Sofiritari Amgbara <sofiriamgbara@gmail.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/swissjake/SeamPass-Generator"
  },
  "homepage": "https://github.com/swissjake/SeamPass-Generator#readme",
  "license": "MIT",
  "devDependencies": {
    "@seampass/eslint-config": "workspace:*",
    "@seampass/typescript-config": "workspace:*",
    "@types/node": "^22.15.3",
    "eslint": "^9.30.0",
    "tsup": "^8.2.4",
    "typescript": "^5.6.3"
  },
  "dependencies": {
    "@seampass/shared": "workspace:*",
    "@seampass/core": "workspace:*"
  }
}
